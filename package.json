{
  "name": "smart-academy",
  "version": "2.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href / --proxy-config proxy.conf.json",
    "start:prod": "ng serve --prod --base-href / ",
    "build:prod": "ng build --prod  --base-href /",
    "postbuild:prod": "git checkout master && git merge develop && git push && git push --tags && git checkout develop",
    "test": "ng test",
    "prebuild:stg": "git checkout develop && git pull",
    "build:stg": "npm version",
    "postbuild:stg": "git push && git push --tags && npm run extract-ar-translation && npm run extract-en-translation && ng build --base-href / && firebase deploy",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --format json",
    "extract-translations": "npm run extract-ar-translation && extract-en-translation",
    "extract-ar-translation": "ngx-translate-extract --input  ./src --output -o ./src/assets/i18n/ar.json -k --clean --sort --format namespaced-json --marker _",
    "extract-en-translation": "ngx-translate-extract --input  ./src --output -o ./src/assets/i18n/en.json  -k  --clean --sort --format namespaced-json --marker _"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/localize": "~9.0.3",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "@angular/service-worker": "~9.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@vimeo/player": "^2.12.2",
    "bootstrap": "^4.5.2",
    "html2canvas": "^1.0.0-rc.7",
    "ngx-bootstrap": "^5.6.1",
    "ngx-captcha": "^8.0.1",
    "ngx-confirm-box": "0.0.3",
    "ngx-device-detector": "^1.4.6",
    "ngx-export-as": "^1.4.2",
    "ngx-feedback": "^1.0.0",
    "ngx-infinite-scroll": "^9.0.0",
    "ngx-linkifyjs": "^1.3.0",
    "ngx-loading": "^8.0.0",
    "ngx-mat-select-search": "^3.2.0",
    "ngx-progressbar": "^6.0.3",
    "ngx-spinner": "^9.0.2",
    "ngx-toastr": "^12.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "vimeo": "^2.1.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.50",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
